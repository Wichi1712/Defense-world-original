            <string>///Estados Boss

switch (global.estadoBoss)
{
    case "stop":
        scr_enterBoss();
    break;
    
    case "war":
        obj_tanque.activo = true;//activa movimiento de player
        
        switch (faseAtaque)
        {
        
            //fase cero
            case 0:
                if(instance_exists(par_balaplayer)) faseAtaque++;
            break;
            
            //primera fase
            case 1:
                speed = 2;
                //Crea disparo
                if (instance_number( obj_balaBoss)&lt;3 and place_free (x , y +5))
                     instance_create( x, y, obj_balaBoss);
             
                 if global.saludBoss &lt;= 70 faseAtaque++;
             
            break;
    
             //segunda fase
            case 2:
                //speed = 3;
                if (instance_number( obj_balaBoss)&lt;6 and place_free (x , y +5))
                    instance_create( x, y, obj_balaBoss);
                    
                if global.saludBoss &lt;= 50 faseAtaque++;
             
            break;
            
            //tercera fase 
            case 3:                  
                if (instance_number( obj_balaOvni)&lt;=4 and place_free( x, y + 15))
                    instance_create( x, y + 2, obj_balaOvni);
                    
                if global.saludBoss &lt;= 30 faseAtaque++;
            
            break;
            
            //cuarta fase
            case 4: 
                //speed = 3;     
                if (instance_number( obj_balaOvni)&lt;=7 and place_free( x, y + 15))
                    instance_create( x, y + 2, obj_balaOvni);
                    
                if global.saludBoss &lt;= 0 faseAtaque++;
                
            break;
            
            case 5://Vencido
                scr_vencido();
                //if ( obj_control.alarm[1]&lt;= room_speed * 2){ instance_destroy();}

            break;
        }
    break;
}
   

